<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_famille_tree" model="ir.ui.view">
        <field name="name">patrimoine.famille.tree</field>
        <field name="model">patrimoine.famille</field>
        <field name="arch" type="xml">
            <tree>
                <field name="libelle"/>
            </tree>
        </field>
    </record>

    <record id="view_famille_form" model="ir.ui.view">
        <field name="name">patrimoine.famille.form</field>
        <field name="model">patrimoine.famille</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="libelle"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
