<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_marque_tree" model="ir.ui.view">
        <field name="name">patrimoine.marque.tree</field>
        <field name="model">patrimoine.marque</field>
        <field name="arch" type="xml">
            <tree>
                <field name="libelle"/>
            </tree>
        </field>
    </record>

    <record id="view_marque_form" model="ir.ui.view">
        <field name="name">patrimoine.marque.form</field>
        <field name="model">patrimoine.marque</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="libelle"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
